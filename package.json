{
  "name": "blockify-plugin",
  "version": "0.0.4",
  "description": "Blockify plugin.",
  "author": "Blockify",
  "license": "GPL-2.0-or-later",
  "main": "assets/js/index.js",
  "scripts": {
    "build": "npm run js && npm run css && npm run pot",
    "js": "wp-scripts build",
    "css": "rm -Rf assets/css && node-sass --output-style compressed --recursive -o assets/css src/scss && rm -Rf assets/css/abstracts",
    "pot": "wp i18n make-pot ./ languages/blockify.pot --exclude='wp,wp-content,vendor,tests,node_modules,build,assets,src'",
    "test": "jest --verbose ./tests/js/*.js",
    "zip": "rm -Rf ../../plugin.zip && zip -r ../../plugin.zip assets/ build/ includes/ languages/ patterns/ styles/ vendor/ plugin.php",
    "svg": "node node_modules/oslllo-svg-fixer/src/cli.js -s ./assets/logos -d ./assets/logos-op"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.18.6",
    "@types/redux": "^3.6.0",
    "@types/wordpress__block-editor": "^7.0.0",
    "@types/wordpress__components": "^19.10.3",
    "@wordpress/api-fetch": "^6.16.0",
    "@wordpress/block-editor": "^8.0.13",
    "@wordpress/blocks": "^11.16.0",
    "@wordpress/components": "^19.2.0",
    "@wordpress/compose": "^5.0.7",
    "@wordpress/data": "^6.3.0",
    "@wordpress/dom-ready": "^3.6.0",
    "@wordpress/edit-post": "^6.3.0",
    "@wordpress/edit-site": "^4.15.0",
    "@wordpress/editor": "^12.5.1",
    "@wordpress/element": "^4.1.1",
    "@wordpress/hooks": "^3.6.0",
    "@wordpress/i18n": "^4.19.0",
    "@wordpress/icons": "^9.10.0",
    "@wordpress/plugins": "^4.16.0",
    "@wordpress/primitives": "^3.17.0",
    "@wordpress/scripts": "^21.0.1",
    "@wordpress/server-side-render": "^3.17.0",
    "browser-sync-webpack-plugin": "^2.3.0",
    "html-react-parser": "^1.4.14",
    "jest": "^29.0.3",
    "node-sass": "^7.0.1",
    "oslllo-svg-fixer": "^2.2.0",
    "ts-node": "^10.9.1"
  }
}
