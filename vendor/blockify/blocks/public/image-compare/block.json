{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "blockify/image-compare",
  "title": "Image Compare",
  "description": "Simple comparison slider for comparing before and after versions of an image.",
  "icon": "flip-horizontal",
  "textdomain": "blockify-blocks",
  "keywords": [
    "comparison"
  ],
  "attributes": {
    "beforeImage": {
      "type": "object"
    },
    "afterImage": {
      "type": "object"
    },
    "style": {
      "type": "object"
    }
  },
  "supports": {
    "__experimentalBorder": {
      "width": true,
      "color": true,
      "style": true,
      "radius": true
    }
  },
  "editorScript": "file:index.js",
  "style": "file:view.css",
  "viewScript": "file:view.js",
  "editorStyle": "file:index.css"
}