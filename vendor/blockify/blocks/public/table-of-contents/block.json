{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "blockify/table-of-contents",
  "title": "Table of Contents",
  "description": "A block that displays an automatically generated table of contents for the current page.",
  "icon": "list-view",
  "textdomain": "blockify-blocks",
  "keywords": [
    "toc",
    "table of contents",
    "links"
  ],
  "supports": {
    "typography": {
      "fontSize": true,
      "lineHeight": true,
      "letterSpacing": true,
      "textDecoration": true,
      "fontWeight": true
    },
    "spacing": {
      "blockGap": true
    }
  },
  "editorScript": "file:index.js",
  "editorStyle": "file:index.css",
  "style": "file:view.css"
}