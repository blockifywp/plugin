{
    "packages": [
        {
            "name": "blockify/theme",
            "version": "dev-main",
            "version_normalized": "dev-main",
            "dist": {
                "type": "path",
                "url": "../../themes/blockify",
                "reference": "0c5ca209c498441416b872588be7905d2c4c7520"
            },
            "require": {
                "php": ">=7.4"
            },
            "require-dev": {
                "composer/installers": "^1.5 || ^2.0",
                "dealerdirect/phpcodesniffer-composer-installer": "^0.7",
                "dingo-d/wp-pest": "dev-main",
                "mockery/mockery": "^1.6@dev",
                "php-parallel-lint/php-parallel-lint": "^1.3",
                "szepeviktor/phpcs-psr-12-neutron-hybrid-ruleset": "dev-master",
                "szepeviktor/phpstan-wordpress": "^1.1",
                "wp-coding-standards/wpcs": "^2.3"
            },
            "type": "library",
            "installation-source": "dist",
            "autoload": {
                "files": [
                    "functions.php"
                ]
            },
            "scripts": {
                "install:dev": [
                    "@composer update"
                ],
                "install:prod": [
                    "@composer update --no-dev"
                ],
                "install:cs": [
                    "Dealerdirect\\Composer\\Plugin\\Installers\\PHPCodeSniffer\\Plugin::run"
                ],
                "install:pest": [
                    "pest --init"
                ],
                "phpcs": [
                    "phpcs -s"
                ],
                "phpcbf": [
                    "phpcbf"
                ],
                "phpstan": [
                    "phpstan --memory-limit=1G"
                ],
                "pest": [
                    "pest"
                ]
            },
            "license": [
                "GPL-2.0-or-later"
            ],
            "authors": [
                {
                    "name": "Blockify",
                    "homepage": "https://blockifywp.com"
                }
            ],
            "description": "Blockify WordPress Theme",
            "homepage": "https://wordpress.org/themes/blockify/",
            "keywords": [
                "blockify",
                "blocks",
                "fse",
                "theme",
                "wordpress",
                "wp"
            ],
            "support": {
                "issues": "https://github.com/blockifywp/theme/issues"
            },
            "transport-options": {
                "symlink": false,
                "relative": true
            },
            "install-path": "../blockify/theme"
        }
    ],
    "dev": false,
    "dev-package-names": []
}
