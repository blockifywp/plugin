                "phpcbf": [
                    "phpcbf"
                ],
                "phpstan": [
                    "phpstan"
                ],
                "pest": [
                    "pest"
                ]
            },
            "license": [
                "GPL-2.0-or-later"
            ],
            "authors": [
                {
                    "name": "Blockify",
