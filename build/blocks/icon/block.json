{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "blockify/icon",
  "title": "Icon",
  "description": "Lightweight, fully customizable SVG icon block. Default icon sets include Dashicons, Social icons and WordPress editor icons. Renders an inline SVG, no icon fonts are loaded. Supports gradients and shadows.",
  "icon": "star-empty",
  "category": "blockify",
  "textdomain": "blockify",
  "keywords": [
    "font awesome",
    "fontawesome",
    "dashicons",
    "svg"
  ],
  "supports": {
    "align": false,
    "alignWide": false,
    "boxShadow": true,
    "spacing": {
      "margin": true,
      "padding": true
    },
    "__experimentalBorder": {
      "width": true,
      "style": true,
      "color": true,
      "radius": true,
      "__experimentalDefaultControls": {
        "width": true,
        "color": true
      }
    },
    "__experimentalLayout": {
      "allowSwitching": false,
      "allowInheriting": false,
      "default": {
        "type": "flex"
      }
    }
  },
  "attributes": {
    "iconSet": {
      "type": "string",
      "default": "wordpress"
    },
    "icon": {
      "type": "string",
      "default": "star-empty"
    },
    "svgString": {
      "type": "string",
      "default": "<svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 24 24\"><path fill-rule=\"evenodd\" d=\"M9.706 8.646a.25.25 0 01-.188.137l-4.626.672a.25.25 0 00-.139.427l3.348 3.262a.25.25 0 01.072.222l-.79 4.607a.25.25 0 00.362.264l4.138-2.176a.25.25 0 01.233 0l4.137 2.175a.25.25 0 00.363-.263l-.79-4.607a.25.25 0 01.072-.222l3.347-3.262a.25.25 0 00-.139-.427l-4.626-.672a.25.25 0 01-.188-.137l-2.069-4.192a.25.25 0 00-.448 0L9.706 8.646zM12 7.39l-.948 1.921a1.75 1.75 0 01-1.317.957l-2.12.308 1.534 1.495c.412.402.6.982.503 1.55l-.362 2.11 1.896-.997a1.75 1.75 0 011.629 0l1.895.997-.362-2.11a1.75 1.75 0 01.504-1.55l1.533-1.495-2.12-.308a1.75 1.75 0 01-1.317-.957L12 7.39z\" clip-rule=\"evenodd\"> </path></svg>"
    },
    "iconWidth": {
      "type": "string"
    },
    "iconColor": {
      "type": "string",
      "default": "var(--wp--preset--color--primary-600)"
    },
    "iconGradient": {
      "type": "string"
    },
    "backgroundColor": {
      "type": "string"
    },
    "backgroundGradient": {
      "type": "string"
    },
    "link": {
      "type": "string"
    },
    "flipHorizontal": {
      "type": "boolean"
    },
    "flipVertical": {
      "type": "boolean"
    },
    "style": {
      "type": "object",
      "default": {
        "border": {
          "radius": "var(--wp--custom--border--radius)"
        }
      }
    }
  },
  "editorScript": "file:index.js",
  "script": "file:script.js",
  "editorStyle": "file:editor.css",
  "style": "file:style.css"
}